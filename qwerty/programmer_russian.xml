<?xml version="1.0" encoding="utf-8"?>
<keyboard name="Programmer (Русский)" script="cyrillic">
  <row>
    <key c="й" nw="loc esc" se="@" ne="1"/>
    <key c="ц" se="-" ne="2"/>
    <key c="у" se="!" ne="3"/>
    <key c="к" sw="$" ne="4"/>
    <key c="е" sw="%" nw="5" ne="ё"/>
    <key c="н" sw="`" ne="loc redo"/>
    <key c="г" se="^" ne="6"/>
    <key c="ш" se="&amp;" ne="7"/>
    <key c="щ" sw="*" ne="8"/>
    <key c="з" sw="_" ne="9"/>
    <key c="х" sw="'" nw="0" ne="loc page_up"/>
  </row>
  <row>
    <key c="ф" se="[" nw="loc tab" sw="loc selectAll"/>
    <key c="ы" se="("/>
    <key c="в" nw="{"/>
    <key c="а" nw="&quot;"/>
    <key c="п" nw=";"/>
    <key c="р" nw="\\"/>
    <key c="о" ne="+"/>
    <key c="л" ne="="/>
    <key c="д" ne="}"/>
    <key c="ж" sw=")"/>
    <key c="э" ne="|" sw="]" se="loc page_down"/>
  </row>
  <row>
    <key width="1.18" c="shift" ne="loc capslock"/>
    <key width="0.96" c="я" ne="&lt;" sw="loc undo"/>
    <key width="0.96" c="ч" ne="," sw="loc cut"/>
    <key width="0.96" c="с" nw=":" sw="loc copy"/>
    <key width="0.96" c="м" nw="#" sw="loc paste" se="loc pasteAsPlainText"/>
    <key width="0.96" c="и" nw="/"/>
    <key width="0.96" c="т" ne="~"/>
    <key width="0.96" c="ь" ne="?" se="ъ"/>
    <key width="0.96" c="б" nw="."/>
    <key width="0.96" c="ю" nw="&gt;"/>
    <key width="1.18" c="backspace" ne="delete"/>
  </row>
</keyboard>
