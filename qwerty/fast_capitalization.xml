<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<keyboard name="QWERTY (Fast Capitalization)" script="latin">
  <row>
    <key key0="q" key1="Q" key2="1" key4="esc"/>
    <key key0="w" key1="W" key2="2" key3="\@" key4="~"/>
    <key key0="e" key1="E" key2="3" key3="\#" key4="!"/>
    <key key0="r" key1="R" key2="4" key3="$"/>
    <key key0="t" key1="T" key2="5" key3="%"/>
    <key key0="y" key1="Y" key2="6" key3="^"/>
    <key key0="u" key1="U" key2="7" key3="&amp;"/>
    <key key0="i" key1="I" key2="8" key3="*"/>
    <key key0="o" key1="O" key2="9" key3="(" key4=")"/>
    <key key0="p" key1="P" key2="0"/>
  </row>
  <row>
    <key key0="alt" key1="esc" key4=":"/>
    <key shift="0.5" key0="a" key1="A" key2="`" key4="tab"/>
    <key key0="s" key1="S" key2="loc §" key3="loc ß"/>
    <key key0="d" key1="D"/>
    <key key0="f" key1="F"/>
    <key key0="g" key1="G" key2="-" key3="_"/>
    <key key0="h" key1="H" key2="=" key3="+"/>
    <key key0="j" key1="J" key3="{" key4="}"/>
    <key key0="k" key1="K" key3="[" key4="]"/>
    <key key0="l" key1="L" key2="|" key3="\\"/>
  </row>
  <row>
    <key width="1.5" key0="shift" key2="capslock"/>
    <key key0="z" key1="Z"/>
    <key key0="x" key1="X" key2="loc †"/>
    <key key0="c" key1="C" key2="&lt;" key3="."/>
    <key key0="v" key1="V" key2=">" key3=","/>
    <key key0="b" key1="B" key2="\?" key3="/"/>
    <key key0="n" key1="N" key2=":" key3=";"/>
    <key key0="m" key1="M" key2="&quot;" key3="'"/>
    <key width="1.5" key0="backspace" key2="delete"/>
  </row>
</keyboard>
